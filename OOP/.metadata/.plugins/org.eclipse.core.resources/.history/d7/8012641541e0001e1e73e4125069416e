package labs1_ex;

public class Book {
	private String title;
	private int ISBN;
	private int publicationYear;
	private Author authour;
	private boolean status;

	public Book(String title, int iSBN, int publicationYear, Author authour, boolean status) {
		super();
		this.title = title;
		ISBN = iSBN;
		this.publicationYear = publicationYear;
		this.authour = authour;
		this.status = status;
	}
	
	

}
