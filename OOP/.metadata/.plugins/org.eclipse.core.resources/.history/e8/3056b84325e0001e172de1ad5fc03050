package labs1_ex;

public class Book {
	private String title;
	private int ISBN;
	private int publicationYear;
	private Author authour;

	public Book(String title, int iSBN, int publicationYear, Author authour) {
		super();
		this.title = title;
		this.ISBN = iSBN;
		this.publicationYear = publicationYear;
		this.authour = authour;
	}
	

}
